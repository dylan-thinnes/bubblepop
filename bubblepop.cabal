  build-depends:
    base >=4.12 && <4.13,
    safe,
    recursion-schemes,
    data-fix,
    megaparsec,
    text,
    containers,
    bifunctors,
    free,
    deriving-compat
  -- hs-source-dirs:
  default-language:    Haskell2010
  ghc-options:
    -W -Wno-unused-imports
