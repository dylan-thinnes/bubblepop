build-type:          Simple
extra-source-files:  CHANGELOG.md

executable bubblepop
  main-is:             Main.hs
  other-modules:
    LearnParsing
    Experiment.Data
    Experiment.Parser
    Experiment.Flat
    Experiment.Simple
    Experiment.Fix2
    Experiment.Carrier
    Experiment.Hatches
    Experiment.Cooperate
    Experiment.Utils
    Experiment.Patterns
    Experiment.PatternsWOReplaceLet
    Experiment.Zipper
    Experiment.ZipperFamilies
